] = [];\n    date: Date;\n    matricula = '';\n    date1: Date;\n    date2: Date;\n    toUpdate = false;\n    fUpdateId = 0;\n    \n\n    selectedCarFuel: number;\n    selectedCarFuelDate: number;\n\n    cars: any[] = [];\n\n \n  constructor(private readonly _fuelService:FuelService,  private toastr: ToastrService) {\n    //this.downloadPDF();\n  }\n\n  ngOnInit(): void {\n    this.loadFuel();\n    this.loadCars();\n    this.sumFuels();\n  }\n\n  public downloadPDF(): void {\n    const doc = new jsPDF();\n\n    doc.text('Hello world!', 10, 10);\n    doc.save('hello-world.pdf');\n  }\n  createFuel() {\n\n    this.fuel.car = this.selectedCarFuel;\n    this.fuel.ic = this.fuel.car.ic;\n    return this._fuelService.createFuel(this.fuel).subscribe((res: any) => {\n      this.toastr.success(res.message);\n      this.loadFuel();\n      this.fuel.odom = null;\n      this.fuel.controlDate = null;\n      this.fuel.fuelByDriver = null;\n      this.fuel.fuelAdd = null;\n      this.selectedCarFuel = null;\n      this.fuel.fuelInTanqReal = null;\n      this.fuel.fuelInTarget = null;\n      this.fuel.fuelRechargued = null;\n    });\n\n  }\n\n  removeFuel(id:number) {\n    \n    let fDelete = this.fuels[id];\n    return this._fuelService.deleteFuel(fDelete.id).subscribe((res: any) => {\n      this.loadFuel();\n    });\n\n  }\n\n  loadFuel() {\n    return this._fuelService.getFuel().subscribe((res: any) => {\n      this.fuels = res;\n      console.log(this.fuels);\n      \n    });\n  }\n\n  loadFuelByCarDate() {\n    return this._fuelService.getFuelMatDate(this.selectedCarFuelDate,this.date1, this.date2).subscribe((res: any) => {\n      this.fuels = res;\n      console.log(this.fuels);\n      \n    });\n  }\n\n  sumFuels(){\n    this.loadFuel();\n    let faux: Fuel = new Fuel();\n   \n    this.fuelsAux = this.fuels;\n    for (let index = 0; index < this.fuelsAux.length; index++){\n      faux = this.fuelsAux[index];\n      for (let i = index + 1; i < this.fuelsAux.length; i++) {\n        if(faux.car === this.fuelsAux[i].car){\n          faux.fuelByDriver = faux.fuelByDriver + this.fuelsAux[i].fuelByDriver;\n        }\n      }\n      this.fuelsFinal.push(faux);\n    }\n    \n    return this.fuelsFinal;\n  }\n\n  loadCars() {\n    return this._fuelService.getCar().subscribe((res: any) => {\n      this.cars = res;\n          \n    });\n  }\n\n  updateFuel(id:number) {\n    this.toUpdate = true;\n    let fUpdate = this.fuels[id];\n    this.fUpdateId = fUpdate.id;\n   \n    this.fuel.odom = fUpdate.odom;\n    this.fuel.controlDate = fUpdate.controlDate;\n    this.fuel.fuelByDriver = fUpdate.fuelByDriver;\n   \n    this.fuel.fuelInTarget = fUpdate.fuelInTarget;\n    this.fuel.fuelRechargued = fUpdate.fuelRechargued;\n    this.fuel.fuelAdd = fUpdate.fuelAdd;\n    this.fuel.fuelInTanqReal = fUpdate.fuelInTanqReal;\n    this.fuel.isCertified = fUpdate.isCertified;\n    this.fuel.waybillNumber = fUpdate.waybillNumber;\n   \n  }\n\n  updatedFuel(fUpdateId){\n    \n    this.fuel.car = this.selectedCarFuel;\n    \n      return this._fuelService.updateFuel(fUpdateId, this.fuel).subscribe((res: any) => {\n      this.loadFuel();\n      this.toastr.success(res.message);\n      this.fuel.odom = null;\n      this.fuel.controlDate = null;\n      this.fuel.fuelByDriver = null;\n      this.selectedCarFuel = null;\n   \n      this.fuel.fuelInTarget = null;\n      this.fuel.fuelRechargued = null;\n      this.fuel.fuelAdd = null;\n      this.fuel.fuelInTanqReal = null;\n      this.fuel.isCertified = false;\n      this.fuel.waybillNumber = null;\n      this.fuel.description = null;\n      this.toUpdate = false;\n    });\n  }\n\n  getValues() {\n    this.selectedCarFuel = this.selectedCarFuel;\n   \n  }\n\n  onChange = ($event: any): void => {\n    console.log($event);\n    console.log(`SELECTION CHANGED INTO ${$event.mat || ''}`);\n  }\n\n  \n}\n","export default \"<!-- Navbar -->\\n<nav\\n  class=\\\"absolute top-0 left-0 w-full z-10 bg-transparent md:flex-row md:flex-