a la api.\n      });\n    } catch (error) {\n      \n    }\n  }\n\n  loadPlans(){\n    return this._dPlanService.findAll().subscribe((res:any[])=>{\n      this.plans = res;\n      console.log(this.plans);\n    })\n  }\n\n  loadReports(){\n    return this._dReportService.findAll().subscribe((res:any[])=>{\n      this.reports = res;\n      \n    })\n  }\n\n  loadContracts(){\n    return this._dContractService.findAll().subscribe((res:any[])=>{\n      this.contracts = res;\n      \n      console.log(this.contracts);\n    })\n  }\n\n  loadSupplements(){\n    return this._dSupplementService.findAll().subscribe((res:any[])=>{\n      this.supplements = res;\n      \n    })\n  }\n\n  loadInspections(){\n    return this._dInspectionService.findAll().subscribe((res:any[])=>{\n      this.inspections = res;\n      \n    })\n  }\n\n  loadOthers(){\n    return this._dOtherService.findAll().subscribe((res:any[])=>{\n      \n      this.others = res;\n      \n    })\n  }\n\n  downloadDoc(id:number){\n     const idDownload:number = this.inspections[id].id;\n    \n    return this._documentService.download(60).subscribe((res:any)=>{\n      \n    })\n  }\n  \n\n  deleteContract(id:number){\n    const docDeleteId = this.contracts[id].id;\n    Swal.fire({\n      title: 'Estás seguro?',\n      text: \"Desea eliminar el documento!\",\n      icon: 'warning',\n      showCancelButton: true,\n      confirmButtonColor: '#3085d6',\n      cancelButtonColor: '#d33',\n      confirmButtonText: 'Sí, eliminarlo!'\n    }).then((result) => {\n      if (result.isConfirmed) {\n        return this._documentService.delete(docDeleteId).subscribe((res:any)=>{\n          this.toastr.success(res.message);\n          this.loadContracts();\n        })\n        \n      }\n    })\n    \n  }\n\n  deletePlan(id:number){\n    const docDeleteId = this.plans[id].id;\n    Swal.fire({\n      title: 'Estás seguro?',\n      text: \"Desea eliminar el documento!\",\n      icon: 'warning',\n      showCancelButton: true,\n      