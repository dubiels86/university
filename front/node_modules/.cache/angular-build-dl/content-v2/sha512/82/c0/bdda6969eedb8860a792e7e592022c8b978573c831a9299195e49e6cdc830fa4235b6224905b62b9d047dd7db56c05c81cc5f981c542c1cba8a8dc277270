toastr.success(res.message);\n      this.findAll();\n      this.waste.name = null;    \n      this.waste.description = null;  \n    }, ex => {\n      if(ex.error.status===400){\n        Swal.fire({\n          icon: 'error',\n          title: 'Oops...',\n          text: 'No puede estar vacío el campo desecho!' \n        })\n      }\n    }\n    );\n\n  }\n\n  remove(id:number) {\n    \n      let wDelete:Waste = this.wastes[id];\n    \n      return this._wasteService.delete(wDelete.code).subscribe((res: any) => {\n        debugger\n        this.wastes = res.obj; \n        this.findAll();\n      }, ex => {\n        if(ex.error.status===500){\n          Swal.fire({\n            icon: 'error',\n            title: 'Oops...',\n            text: 'No puede eliminar el desecho por estar asociado algún plan existente!' \n          })\n        }  \n        \n      });\n    \n   \n\n  }\n\n  findAll() {\n    return this._wasteService.findAll().subscribe((res: any) => {\n      this.wastes = res;      \n    });\n  }\n\n  update(id:number) {\n    this.toUpdate = true;\n    let wUpdate = this.wastes[id];\n    this.wUpdateId = wUpdate.code;\n   \n    this.waste.name = wUpdate.name;\n    this.waste.description = wUpdate.description;\n  }\n\n  updated(wUpdateId){\n      return this._wasteService.update(wUpdateId, this.waste).subscribe((res: any) => {\n      this.findAll();\n      this.toastr.success(res.message);\n      this.waste.name = null;\n      this.waste.description = null;\n      this.toUpdate = false;\n    });\n  }\n  \n}\n","export default \"<div class=\\\"flex flex-wrap mt-4\\\">\\n  <div class=\\\"w-full mb-12 px-4\\\">\\n    <app-card-table></app-card-table>\\n  </div>\\n  <div class=\\\"w-full mb-12 px-4\\\">\\n    <app-card-table color=\\\"dark\\\"></app-card-table>\\n  </div>\\n</div>\\n\";","import { Component, OnInit } from \"@angular/core\";\n\n@Component({\n  selector: \"app-dashboard-menu\",\n  templateUrl: \"./dashboard-menu.component.html\",\n})\nexport class Dash