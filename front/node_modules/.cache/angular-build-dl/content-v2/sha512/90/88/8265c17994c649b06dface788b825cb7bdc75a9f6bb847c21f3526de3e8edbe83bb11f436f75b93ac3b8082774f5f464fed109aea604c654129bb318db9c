          }
        }, {
          key: "getFuelById",
          value: function getFuelById(id) {
            return this.httpClient.get("".concat(this.API, "/").concat(id));
          }
        }, {
          key: "getFuelMatDate",
          value: function getFuelMatDate(id, date1, date2) {
            return this.httpClient.get("".concat(this.API, "/byCarAndDate/").concat(id, "/").concat(date1, "/").concat(date2));
          }
        }, {
          key: "getStatsByCar",
          value: function getStatsByCar(id, date1, date2) {
