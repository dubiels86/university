      var MenuService = /*#__PURE__*/function () {
        function MenuService(httpClient) {
          _classCallCheck(this, MenuService);

          this.httpClient = httpClient;
          this.API = 'http://localhost:3000/api/products';
        }

        _createClass(MenuService, [{
          key: "getProducts",
          value: function getProducts() {
            return this.httpClient.get(this.API);
          }
        }, {
          key: "getProductById",
          value: function getProductById(id) {
            return this.httpClient.get("".concat(this.API, "/").concat(id));
          }
        }, {
          key: "createProduct",
          value: function createProduct(product) {
            return this.httpClient.post(this.API, product);
          }
        }, {
          key: "deleteProduct",
          value: function deleteProduct(id) {
            return this.httpClient["delete"]("".concat(this.API, "/").concat(id));
          }
        }, {
          key: "updateProduct",
          value: function updateProduct(id, name, description) {
            return this.httpClient.put("".concat(this.API, "/").concat(id), {
              name: name,
              description: description
            });
          }
        }]);

        return MenuService;
      }();

