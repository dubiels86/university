            var action = arguments.length > 0 && arguments[0] !== undefined ? arguments[0] : 'open';
            this.generateConsolidatedReport();
            debugger;
            var month = new Date().getUTCMonth() + 1;
            var docDefinition = {
              pageOrientation: 'landscape',
              pageSize: 'A4',
              footer: {
                columns: [//[{ qr: `${this.invoice.customerName}`, fit: '50' }],
                [{
                  text: 'Elaborado por:' + ' ' + "".concat(this.consolidateReport.draftedBy),
                  alignment: 'left',
                  italics: true,
                  margin: [38, 2, 10, 20]
                }], [{
                  text: 'Cierre:' + ' ' + "".concat(this.consolidateReport.closingDate),
                  alignment: 'right',
                  italics: true,
                  margin: [5, 2, 38, 20]
                }]]
              },
              content: [
              /*{
                text: 'ELECTRONIC SHOP',
                fontSize: 16,
                alignment: 'center',
                color: '#047886'
              },
              {
                text: 'INVOICE',
                fontSize: 20,
                bold: true,
                alignment: 'center',
                decoration: 'underline',
                color: 'skyblue'
              },*/

              /*{
                  image: 'data:image/jpeg;base64,',
                  width: 120,
                
                style: 'sectionHeader'
              },*/
              {
                columns: [
                /*[
                  {
                    text: this.invoice.customerName,
                    bold:true
                  },
                  { text: this.invoice.address },
                  { text: this.invoice.email },
                  { text: this.invoice.contactNo }
                ],*/
                [{
                  text: new Date().getDate() + '/' + month + '/' + new Date().getFullYear(),
                  alignment: 'right'
                }]]
              }, {
                text: this.consolidateReport.title + '.' + '' + this.consolidateReport.consolidatedPeriodFinalMonth
              }, {
                table: {
                  headerRows: 1,
                  widths: ['*', 'auto', 'auto', 'auto', 'auto', 'auto', 'auto', 'auto', 'auto', 'auto', 'auto'],
                  body: [['Desecho', 'I Trim. Recic.', 'I Trim. Recuper.', 'II Trim. Recic.', 'II Trim. Recuper.', 'III Trim. Recic.', 'III Trim. Recuper.', 'IV Trim. Recic.', 'IV Trim. Recuper.', 'Total Recic.', 'Total Recuper.']].concat(_toConsumableArray(this.consolidateReport.details.map(function (r) {
                    return [r.waste, r.irecycledAmount, r.irecoveredAmount, r.iirecycledAmount, r.iirecoveredAmount, r.iiirecycledAmount, r.iiirecoveredAmount, r.ivrecycledAmount, r.ivrecoveredAmount, r.totalRecycledAmount, r.totalRecoveredAmount];
                  })))
                }
              }],
              styles: {
                sectionHeader: {
                  bold: true,
                  decoration: 'underline',
                  fontSize: 14,
                  margin: [0, 15, 0, 15]
                }
              }
            };

            if (action === 'download') {
              pdfmake_build_pdfmake__WEBPACK_IMPORTED_MODULE_13___default.a.createPdf(docDefinition).download();
            } else if (action === 'print') {
              pdfmake_build_pdfmake__WEBPACK_IMPORTED_MODULE_13___default.a.createPdf(docDefinition).print();
            } else {
              pdfmake_build_pdfmake__WEBPACK_IMPORTED_MODULE_13___default.a.createPdf(docDefinition).open();
            }
          }
        }]);

        return CardAddReportComponent;
      }();

