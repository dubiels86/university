            return this.httpClient.get("".concat(this.url, "/").concat(id));
          }
        }, {
          key: "findOneByYearByOffice",
          value: function findOneByYearByOffice(year, office) {
            return this.httpClient.get("".concat(this.url, "/byyearbyoffice/").concat(year, "/").concat(office));
          }
        }, {
          key: "create",
          value: function create(plan, userOffice) {
            return this.httpClient.post("".concat(this.url, "/createtest/").concat(userOffice), plan);
          }
        }, {
          key: "delete",
          value: function _delete(id) {
            return this.httpClient.patch("".concat(this.url, "/").concat(id), '');
          }
        }, {
          key: "update",
          value: function update(id, plan, userOffice) {
            return this.httpClient.patch("".concat(this.url, "/updatetest/").concat(id, "/").concat(userOffice), plan);
          }
        }, {
          key: "confirm",
          value: function confirm(id, office) {
            debugger;
            return this.httpClient.post("".concat(this.url, "/confirm/").concat(id), office);
          }
        }, {
          key: "unconfirm",
