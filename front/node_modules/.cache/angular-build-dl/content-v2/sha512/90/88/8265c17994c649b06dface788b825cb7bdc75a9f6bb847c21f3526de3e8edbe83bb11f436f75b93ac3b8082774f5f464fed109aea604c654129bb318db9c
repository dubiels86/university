          key: "saveRow",
          value: function saveRow(selectedRow) {
            var _iterator = _createForOfIteratorHelper(selectedRow.cells),
                _step;

            try {
              for (_iterator.s(); !(_step = _iterator.n()).done;) {
                var cell = _step.value;

                if ((cell.content == null || cell.content === '') && this.isRequired) {
                  return;
                }
              }
            } catch (err) {
              _iterator.e(err);
            } finally {
              _iterator.f();
            }

            this.service.saveRow(selectedRow);
            var dir = [];

            for (var i = 0; i < selectedRow.cells.length; i++) {
              dir.push(selectedRow.cells[i].content);
            }

            var obj = {
              id: selectedRow.id,
              cells: dir
            };
            this.onSave.emit(obj);
          }
        }, {
          key: "deleteRow",
          value: function deleteRow(selectedRow) {
            this.service.deleteRow(selectedRow);
            var dir = [];

            for (var i = 0; i < selectedRow.cells.length; i++) {
              dir.push(selectedRow.cells[i].content);
            }

            var obj = {
              id: selectedRow.id,
              cells: dir
            };
            this.onRemove.emit(obj);
          }
        }]);

        return EditableTableComponent;
      }();

