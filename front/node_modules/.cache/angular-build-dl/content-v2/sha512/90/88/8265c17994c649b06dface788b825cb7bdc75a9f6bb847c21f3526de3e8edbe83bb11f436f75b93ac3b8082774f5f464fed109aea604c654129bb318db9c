            return this.httpClient["delete"]("".concat(this.API, "/").concat(id));
          }
        }, {
          key: "updateFuel",
          value: function updateFuel(id, fuel) {
            return this.httpClient.patch("".concat(this.API, "/").concat(id), fuel);
          }
