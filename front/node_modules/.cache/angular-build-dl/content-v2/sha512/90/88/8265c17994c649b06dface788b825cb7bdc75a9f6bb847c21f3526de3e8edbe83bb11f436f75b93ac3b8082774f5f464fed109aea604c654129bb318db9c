          value: function getFuelTotal() {
            return this.httpClient.get("".concat(this.API));
          }
        }, {
          key: "createFuel",
          value: function createFuel(fuel) {
            return this.httpClient.post(this.API, fuel);
          }
        }, {
          key: "deleteFuel",
