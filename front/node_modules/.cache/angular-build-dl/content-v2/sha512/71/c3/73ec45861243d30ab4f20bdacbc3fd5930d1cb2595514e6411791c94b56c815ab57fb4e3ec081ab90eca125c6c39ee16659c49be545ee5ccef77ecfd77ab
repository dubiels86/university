    onUpload(id) {
        switch (id) {
            case 1:
                {
                    this.data = {
                        "acronym": this.acronym,
                        "effectiveDate": this.docContract.effectiveDate,
                        "terminationDate": this.docContract.terminationDate,
                        "number": this.docContract.number
                    };
                    let meta = JSON.stringify(this.data);
                    const formData = new FormData();
                    for (let index = 0; index < this.uploadedFiles.length; index++) {
                        formData.append('file', this.uploadedFiles[index]);
                        formData.append('metadata', meta);
                    }
                    this._dContractService.upLoadFile(formData).subscribe((res) => {
                        this.documentUploadedSuccessfuly();
                        this.loadContracts();
                        this.acronym = '';
                        this.docContract.effectiveDate = null;
                        this.docContract.terminationDate = null;
                        this.docContract.number = null;
