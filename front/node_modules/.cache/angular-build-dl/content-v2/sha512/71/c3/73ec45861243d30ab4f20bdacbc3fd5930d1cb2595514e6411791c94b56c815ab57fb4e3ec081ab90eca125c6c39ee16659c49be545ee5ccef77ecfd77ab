/* harmony import */ var ngx_toastr__WEBPACK_IMPORTED_MODULE_6__ = __webpack_require__(/*! ngx-toastr */ "5eHb");







let CardAddFuelComponent = class CardAddFuelComponent {
    constructor(_fuelService, toastr) {
        this._fuelService = _fuelService;
        this.toastr = toastr;
        this.showAlert = false;
        this.fuel = new src_app_model_fuel_entity__WEBPACK_IMPORTED_MODULE_3__["Fuel"]();
        this.fuels = [];
        this.fuelsAux = [];
        this.fuelsFinal = [];
        this.matricula = '';
        this.toUpdate = false;
        this.fUpdateId = 0;
        this.cars = [];
        this.onChange = ($event) => {
            console.log($event);
            console.log(`SELECTION CHANGED INTO ${$event.mat || ''}`);
        };
        //this.downloadPDF();
    }
    ngOnInit() {
        this.loadFuel();
        this.loadCars();
        this.sumFuels();
    }
    downloadPDF() {
        const doc = new jspdf__WEBPACK_IMPORTED_MODULE_5__["default"]();
        doc.text('Hello world!', 10, 10);
        doc.save('hello-world.pdf');
    }
    createFuel() {
        this.fuel.car = this.selectedCarFuel;
        this.fuel.ic = this.fuel.car.ic;
        return this._fuelService.createFuel(this.fuel).subscribe((res) => {
            this.toastr.success(res.message);
            this.loadFuel();
            this.fuel.odom = null;
            this.fuel.controlDate = null;
            this.fuel.fuelByDriver = null;
            this.fuel.fuelAdd = null;
            this.selectedCarFuel = null;
            this.fuel.fuelInTanqReal = null;
            this.fuel.fuelInTarget = null;
            this.fuel.fuelRechargued = null;
        });
    }
    removeFuel(id) {
        let fDelete = this.fuels[id];
        return this._fuelService.deleteFuel(fDelete.id).subscribe((res) => {
            this.loadFuel();
        });
    }
    loadFuel() {
        return this._fuelService.getFuel().subscribe((res) => {
            this.fuels = res;
            console.log(this.fuels);
        });
    }
    loadFuelByCarDate() {
        return this._fuelService.getFuelMatDate(this.selectedCarFuelDate, this.date1, this.date2).subscribe((res) => {
            this.fuels = res;
            console.log(this.fuels);
        });
    }
    sumFuels() {
        this.loadFuel();
        let faux = new src_app_model_fuel_entity__WEBPACK_IMPORTED_MODULE_3__["Fuel"]();
        this.fuelsAux = this.fuels;
        for (let index = 0; index < this.fuelsAux.length; index++) {
            faux = this.fuelsAux[index];
            for (let i = index + 1; i < this.fuelsAux.length; i++) {
                if (faux.car === this.fuelsAux[i].car) {
                    faux.fuelByDriver = faux.fuelByDriver + this.fuelsAux[i].fuelByDriver;
                }
            }
            this.fuelsFinal.push(faux);
        }
        return this.fuelsFinal;
    }
    loadCars() {
        return this._fuelService.getCar().subscribe((res) => {
            this.cars = res;
        });
    }
    updateFuel(id) {
        this.toUpdate = true;
        let fUpdate = this.fuels[id];
        this.fUpdateId = fUpdate.id;
        this.fuel.odom = fUpdate.odom;
        this.fuel.controlDate = fUpdate.controlDate;
        this.fuel.fuelByDriver = fUpdate.fuelByDriver;
        this.fuel.fuelInTarget = fUpdate.fuelInTarget;
        this.fuel.fuelRechargued = fUpdate.fuelRechargued;
        this.fuel.fuelAdd = fUpdate.fuelAdd;
        this.fuel.fuelInTanqReal = fUpdate.fuelInTanqReal;
        this.fuel.isCertified = fUpdate.isCertified;
        this.fuel.waybillNumber = fUpdate.waybillNumber;
    }
    updatedFuel(fUpdateId) {
        this.fuel.car = this.selectedCarFuel;
        return this._fuelService.updateFuel(fUpdateId, this.fuel).subscribe((res) => {
            this.loadFuel();
            this.toastr.success(res.message);
            this.fuel.odom = null;
            this.fuel.controlDate = null;
            this.fuel.fuelByDriver = null;
            this.selectedCarFuel = null;
            this.fuel.fuelInTarget = null;
            this.fuel.fuelRechargued = null;
            this.fuel.fuelAdd = null;
            this.fuel.fuelInTanqReal = null;
            this.fuel.isCertified = false;
            this.fuel.waybillNumber = null;
            this.fuel.description = null;
            this.toUpdate = false;
        });
    }
    getValues() {
        this.selectedCarFuel = this.selectedCarFuel;
    }
};
