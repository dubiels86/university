/***/ "F8gZ":
/*!*******************************************************************!*\
  !*** ./src/app/components/ng-datatable/ng-datatable.component.ts ***!
  \*******************************************************************/
/*! exports provided: EditableTableComponent */
/***/ (function(module, __webpack_exports__, __webpack_require__) {

"use strict";
__webpack_require__.r(__webpack_exports__);
/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "EditableTableComponent", function() { return EditableTableComponent; });
/* harmony import */ var tslib__WEBPACK_IMPORTED_MODULE_0__ = __webpack_require__(/*! tslib */ "mrSG");
/* harmony import */ var _angular_core__WEBPACK_IMPORTED_MODULE_1__ = __webpack_require__(/*! @angular/core */ "fXoL");
/* harmony import */ var _ng_datatable_service__WEBPACK_IMPORTED_MODULE_2__ = __webpack_require__(/*! ./ng-datatable.service */ "sbso");



let EditableTableComponent = class EditableTableComponent {
    constructor(service) {
        this.tableHeaders = [];
        this.tableRows = [];
        this.tableRowsWithId = [];
        this.canDeleteRows = true;
        this.canEditRows = true;
        this.canAddRows = true;
        this.addIcon = 'fa fa-plus';
        this.editIcon = 'fa fa-pencil-square-o';
        this.saveIcon = 'fa fa-check';
        this.deleteIcon = 'fa fa-times';
        this.dataType = [];
        this.errorClass = 'myerror';
        this.isRequired = true;
        this.requiredMessage = 'Campo Requerido';
        this.onSave = new _angular_core__WEBPACK_IMPORTED_MODULE_1__["EventEmitter"]();
        this.onRemove = new _angular_core__WEBPACK_IMPORTED_MODULE_1__["EventEmitter"]();
        this.service = service;
    }
    ngOnInit() {
        if (this.tableRows.length > 0 || (this.tableRows !== undefined && this.tableRowsWithId.length === 0)) {
            this.service.createTable(this.tableHeaders, this.tableRows, this.dataType);
        }
        else if (this.tableRowsWithId.length > 0 || (this.tableRowsWithId !== undefined && this.tableRows.length === 0)) {
            this.service.createTableWithIds(this.tableHeaders, this.tableRowsWithId, this.dataType);
        }
    }
    addRow() {
        this.service.addRow();
    }
    editRow(selectedRow) {
        this.service.editRow(selectedRow);
    }
    cancelEdition(selectedRow) {
        this.service.cancelEdition(selectedRow);
    }
    saveRow(selectedRow) {
        for (const cell of selectedRow.cells) {
            if ((cell.content == null || cell.content === '') && this.isRequired) {
                return;
            }
        }
        this.service.saveRow(selectedRow);
        const dir = [];
        for (let i = 0; i < selectedRow.cells.length; i++) {
            dir.push(selectedRow.cells[i].content);
        }
        const obj = { id: selectedRow.id, cells: dir };
        this.onSave.emit(obj);
    }
    deleteRow(selectedRow) {
        this.service.deleteRow(selectedRow);
        const dir = [];
        for (let i = 0; i < selectedRow.cells.length; i++) {
            dir.push(selectedRow.cells[i].content);
        }
        const obj = { id: selectedRow.id, cells: dir };
        this.onRemove.emit(obj);
    }
};
EditableTableComponent.ctorParameters = () => [
    { type: _ng_datatable_service__WEBPACK_IMPORTED_MODULE_2__["EditableTableService"] }
];
EditableTableComponent.propDecorators = {
    tableHeaders: [{ type: _angular_core__WEBPACK_IMPORTED_MODULE_1__["Input"], args: ['table-headers',] }],
    tableRows: [{ type: _angular_core__WEBPACK_IMPORTED_MODULE_1__["Input"], args: ['table-rows',] }],
    tableRowsWithId: [{ type: _angular_core__WEBPACK_IMPORTED_MODULE_1__["Input"], args: ['table-rows-with-id',] }],
    canDeleteRows: [{ type: _angular_core__WEBPACK_IMPORTED_MODULE_1__["Input"], args: ['can-delete-rows',] }],
    canEditRows: [{ type: _angular_core__WEBPACK_IMPORTED_MODULE_1__["Input"], args: ['can-edit-rows',] }],
    canAddRows: [{ type: _angular_core__WEBPACK_IMPORTED_MODULE_1__["Input"], args: ['can-add-rows',] }],
    addButtonLabel: [{ type: _angular_core__WEBPACK_IMPORTED_MODULE_1__["Input"], args: ['add-button-label',] }],
    editButtonLabel: [{ type: _angular_core__WEBPACK_IMPORTED_MODULE_1__["Input"], args: ['edit-button-label',] }],
    saveButtonLabel: [{ type: _angular_core__WEBPACK_IMPORTED_MODULE_1__["Input"], args: ['save-button-label',] }],
