        this.findAllPlans();
        this.generateConsolidatedPlan();
    }
    showEdit() {
        this.toUpdate = !this.toUpdate;
    }
    getDismissReason(reason) {
        if (reason === _ng_bootstrap_ng_bootstrap__WEBPACK_IMPORTED_MODULE_11__["ModalDismissReasons"].ESC) {
            return 'by pressing ESC';
        }
        else if (reason === _ng_bootstrap_ng_bootstrap__WEBPACK_IMPORTED_MODULE_11__["ModalDismissReasons"].BACKDROP_CLICK) {
            return 'by clicking on a backdrop';
        }
        else {
            return `with: ${reason}`;
        }
    }
    detailUpdate() {
        if (this.detail.iRecyclable < 0 || this.detail.iRecoverable < 0 ||
            this.detail.iiRecyclable < 0 || this.detail.iiRecoverable < 0 ||
            this.detail.iiiRecyclable < 0 || this.detail.iiiRecoverable < 0 ||
            this.detail.ivRecyclable < 0 || this.detail.ivRecoverable < 0) {
            sweetalert2__WEBPACK_IMPORTED_MODULE_13___default.a.fire({
                icon: 'error',
                title: 'Oops...',
                text: 'No puede introducir valores negativos!'
            });
        }
        else {
            this.detailsActual[this.detailUpdateId].iRecyclable = this.detail.iRecyclable;
            this.detailsActual[this.detailUpdateId].iiRecyclable = this.detail.iiRecyclable;
            this.detailsActual[this.detailUpdateId].iiiRecyclable = this.detail.iiiRecyclable;
            this.detailsActual[this.detailUpdateId].ivRecyclable = this.detail.ivRecyclable;
            this.detailsActual[this.detailUpdateId].iRecoverable = this.detail.iRecoverable;
            this.detailsActual[this.detailUpdateId].iiRecoverable = this.detail.iiRecoverable;
            this.detailsActual[this.detailUpdateId].iiiRecoverable = this.detail.iiiRecoverable;
            this.detailsActual[this.detailUpdateId].ivRecoverable = this.detail.ivRecoverable;
            this.toUpdate = !this.toUpdate;
            this.toastr.success('Actualizado los datos correspondiente al desecho');
        }
    }
    update(id) {
        if (!this.toUpdate) {
            this.toUpdate = !this.toUpdate;
            this.detailUpdateId = id;
            this.selectWaste = this.detailsActual[id].waste.name;
            this.detail.iRecyclable = this.detailsActual[id].iRecyclable;
            this.detail.iiRecyclable = this.detailsActual[id].iiRecyclable;
            this.detail.iiiRecyclable = this.detailsActual[id].iiiRecyclable;
            this.detail.ivRecyclable = this.detailsActual[id].ivRecyclable;
            this.detail.iRecoverable = this.detailsActual[id].iRecoverable;
            this.detail.iiRecoverable = this.detailsActual[id].iiRecoverable;
            this.detail.iiRecoverable = this.detailsActual[id].iiRecoverable;
            this.detail.iiiRecoverable = this.detailsActual[id].iiiRecoverable;
            this.detail.ivRecoverable = this.detailsActual[id].ivRecoverable;
            this.detail.waste.name = this.detailsActual[id].waste.name;
        }
        else {
            this.toastr.error('Debe guardar los cambios antes de editar otro campo.');
        }
    }
    onRowEditInit(detail) {
        this.clonedDetails[detail.id] = Object.assign({}, detail);
    }
