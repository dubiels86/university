let ReportService = class ReportService {
    constructor(httpClient) {
        this.httpClient = httpClient;
        this.url = _config_config__WEBPACK_IMPORTED_MODULE_3__["Configuration"].apiEndpoint + 'reciclaje/report';
        this.urlReporting = _config_config__WEBPACK_IMPORTED_MODULE_3__["Configuration"].apiEndpoint + 'reciclaje/report/reporting/generate';
    }
    reportingGenerate() {
        debugger;
        return this.httpClient.get(`${this.urlReporting}`);
    }
    findAll() {
        return this.httpClient.get(this.url);
    }
    findOne(id) {
        return this.httpClient.get(`${this.url}/${id}`);
    }
    findOneByYearByOffice(year, office) {
        return this.httpClient.get(`${this.url}/byyearbyoffice/${year}/${office}`);
    }
    create(report, userOffice) {
        return this.httpClient.post(`${this.url}/createtest/${userOffice}`, report);
    }
    update(id, report, userOffice) {
        debugger;
        return this.httpClient.post(`${this.url}/updatetest/${id}/${userOffice}`, report);
    }
