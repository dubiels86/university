/* harmony import */ var _angular_core__WEBPACK_IMPORTED_MODULE_1__ = __webpack_require__(/*! @angular/core */ "fXoL");
/* harmony import */ var _util_table_row__WEBPACK_IMPORTED_MODULE_2__ = __webpack_require__(/*! ./util/table-row */ "M9l5");
/* harmony import */ var _util_table_cell__WEBPACK_IMPORTED_MODULE_3__ = __webpack_require__(/*! ./util/table-cell */ "Qfzx");




let EditableTableService = class EditableTableService {
    constructor() {
        this.tableHeadersObjects = [];
        this.tableRowsObjects = [];
        this.dataType = [];
        this.isEditing = [];
    }
    createTable(headers, content, dataType) {
        this.createHeaders(headers, dataType);
        let tableCells = [];
        if (content.length > 0) {
            for (const row of content) {
                for (const cell of row) {
                    tableCells.push(new _util_table_cell__WEBPACK_IMPORTED_MODULE_3__["TableCell"](cell));
                }
                this.tableRowsObjects.push(new _util_table_row__WEBPACK_IMPORTED_MODULE_2__["TableRow"](tableCells));
                tableCells = [];
            }
        }
    }
    createTableWithIds(headers, content, dataType) {
        this.createHeaders(headers, dataType);
        let tableCells = [];
        if (content.length > 0) {
            for (const row of content) {
                for (let i = 1; i < row.length; i++) {
                    tableCells.push(new _util_table_cell__WEBPACK_IMPORTED_MODULE_3__["TableCell"](row[i]));
                }
                this.tableRowsObjects.push(new _util_table_row__WEBPACK_IMPORTED_MODULE_2__["TableRow"](tableCells, row[0]));
                tableCells = [];
            }
        }
    }
    addRow() {
        const newCells = [];
        let newRow;
        for (let i = 0; i < this.tableHeadersObjects.length; i++) {
            switch (this.dataType[i]) {
                case 'boolean':
                    newCells.push(new _util_table_cell__WEBPACK_IMPORTED_MODULE_3__["TableCell"](false));
                    break;
                default:
                    newCells.push(new _util_table_cell__WEBPACK_IMPORTED_MODULE_3__["TableCell"](''));
            }
        }
        this.tableRowsObjects.push(newRow = new _util_table_row__WEBPACK_IMPORTED_MODULE_2__["TableRow"](newCells, -1));
        this.isEditing.push(newRow);
    }
    editRow(selectedRow) {
        this.isEditing.push(selectedRow);
    }
    saveRow(selectedRow) {
        this.isEditing = this.isEditing.filter(temporalRow => temporalRow !== selectedRow);
    }
    cancelEdition(selectedRow) {
        this.tableRowsObjects = this.tableRowsObjects.filter(temporalRow => temporalRow !== selectedRow);
        this.isEditing = this.isEditing.filter(temporalRow => temporalRow !== selectedRow);
        for (const cell of selectedRow.cells) {
            if (cell.content == null || cell.content === '') {
                this.tableRowsObjects = this.tableRowsObjects.filter(err => err !== selectedRow);
            }
            else if (selectedRow.id === -1) {
                this.tableRowsObjects = this.tableRowsObjects.filter(err => err !== selectedRow);
            }
        }
    }
    deleteRow(selectedRow) {
        this.isEditing = this.isEditing.filter(temporalRow => temporalRow !== selectedRow);
        this.tableRowsObjects = this.tableRowsObjects.filter(temporalRow => temporalRow !== selectedRow);
    }
    checkTypeOf(value) {
        if (typeof (value) === 'boolean') {
            return 'boolean';
        }
        return '';
    }
    createHeaders(headers, dataType) {
        for (const obj of headers) {
            this.tableHeadersObjects.push(new _util_table_cell__WEBPACK_IMPORTED_MODULE_3__["TableCell"](obj));
            this.dataType = dataType;
        }
    }
};
