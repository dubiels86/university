let PlanService = class PlanService {
    constructor(httpClient) {
        this.httpClient = httpClient;
        this.url = _config_config__WEBPACK_IMPORTED_MODULE_3__["Configuration"].apiEndpoint + 'reciclaje/plan';
        this.urlReporting = _config_config__WEBPACK_IMPORTED_MODULE_3__["Configuration"].apiEndpoint + 'reciclaje/plan/reporting/generate';
    }
    findAll() {
        return this.httpClient.get(this.url);
    }
    reportingGenerate(year) {
        return this.httpClient.get(`${this.urlReporting}/${year}`);
    }
    findOne(id) {
        return this.httpClient.get(`${this.url}/${id}`);
    }
    findOneByYearByOffice(year, office) {
        return this.httpClient.get(`${this.url}/byyearbyoffice/${year}/${office}`);
    }
    create(plan, userOffice) {
        return this.httpClient.post(`${this.url}/createtest/${userOffice}`, plan);
    }
    delete(id) {
        return this.httpClient.patch(`${this.url}/${id}`, '');
    }
    update(id, plan, userOffice) {
        return this.httpClient.patch(`${this.url}/updatetest/${id}/${userOffice}`, plan);
    }
    confirm(id, office) {
        debugger;
        return this.httpClient.post(`${this.url}/confirm/${id}`, office);
    }
    unconfirm(id, office) {
        return this.httpClient.post(`${this.url}/unconfirm/${id}`, office);
    }
};
