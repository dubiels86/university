        this.report.rareties = [];
        this.report.rareties = this.rareties;
        return this._reportService.update(this.report.id, this.report, this.officeReport).subscribe((res) => {
            this.reportToUpdate = !this.reportToUpdate;
            this.report.details = [];
            this.report.income = null;
            this.report.month = null;
            this.report.office = null;
            debugger;
            this.report.positionReportedBy = '';
            this.report.rareties = [];
            this.report.reportedBy = '';
            this.report.year = null;
            this.detailsActual = [];
            this.rareties = [];
            this.officeReport = null;
            this.findAllReports();
            this.toastr.success('Reporte actualizado y salvado con éxito');
        });
    }
    updateRefill(id) {
        if (this.reportToUpdate) {
            this.toastr.error('Por favor salvar los cambios del reporte que se esta actualizando!!');
        }
        else {
            sweetalert2__WEBPACK_IMPORTED_MODULE_10___default.a.fire({
                title: 'Estás seguro?',
                text: "Desea actualizar el reporte seleccionado!",
                icon: 'warning',
                showCancelButton: true,
                confirmButtonColor: '#3085d6',
                cancelButtonColor: '#d33',
                confirmButtonText: 'Sí...!'
            }).then((result) => {
                if (result.isConfirmed) {
                    this.report.income = this.reports[id].income;
                    this.report.month = this.reports[id].month;
                    this.officeReport = this.reports[id].office.acronym;
                    this.report.positionReportedBy = this.reports[id].positionReportedBy;
                    this.report.reportedBy = this.reports[id].reportedBy;
                    this.report.year = this.reports[id].year;
                    this.detailsActual = this.reports[id].details;
                    this.rareties = this.reports[id].rareties;
                    this.report.id = this.reports[id].id;
                    this.reportToUpdate = !this.reportToUpdate;
                    this.toastr.success('Los datos del reporte han sido cargados, procede a actualizar los mismos.');
                }
            });
        }
    }
    deleteReport(id) {
        const idToDelete = this.reports[id].id;
        sweetalert2__WEBPACK_IMPORTED_MODULE_10___default.a.fire({
            title: 'Estás seguro?',
            text: "Desea eliminar el reporte!",
            icon: 'warning',
            showCancelButton: true,
            confirmButtonColor: '#3085d6',
            cancelButtonColor: '#d33',
            confirmButtonText: 'Sí, eliminarlo!'
        }).then((result) => {
            if (result.isConfirmed) {
                return this._reportService.delete(idToDelete).subscribe((res) => {
                    this.toastr.success('Eliminado el reporte satisfactoriamente');
                    this.findAllReports();
                });
            }
        });
    }
    //Generate PDF Report/////
    generateConsolidatedReport() {
        return this._reportService.reportingGenerate().subscribe((res) => {
            debugger;
            this.consolidateReport.title = res.title;
            this.consolidateReport.closingDate = res.closingDate;
            this.consolidateReport.comments = res.comments;
            this.consolidateReport.consolidatedPeriodFinalMonth = res.consolidatedPeriodFinalMonth;
            this.consolidateReport.consolidatedPeriodFinalNumberMonth = res.consolidatedPeriodFinalNumberMonth;
            this.consolidateReport.details = res.details;
            this.consolidateReport.draftedBy = res.draftedBy;
            this.consolidateReport.generatedOn = res.generatedOn;
            this.consolidateReport.unitM = res.unitM;
            this.consolidateReport.year = res.year;
            debugger;
        });
    }
    generatePDF(action = 'open') {
        this.generateConsolidatedReport();
        debugger;
        var month = new Date().getUTCMonth() + 1;
        let docDefinition = {
            pageOrientation: 'landscape',
            pageSize: 'A4',
            footer: {
                columns: [
                    //[{ qr: `${this.invoice.customerName}`, fit: '50' }],
                    [{ text: 'Elaborado por:' + ' ' + `${this.consolidateReport.draftedBy}`, alignment: 'left', italics: true, margin: [38, 2, 10, 20] }],
                    [{ text: 'Cierre:' + ' ' + `${this.consolidateReport.closingDate}`, alignment: 'right', italics: true, margin: [5, 2, 38, 20] },],
                ],
            },
            content: [
                /*{
                  text: 'ELECTRONIC SHOP',
                  fontSize: 16,
                  alignment: 'center',
                  color: '#047886'
                },
                {
                  text: 'INVOICE',
                  fontSize: 20,
                  bold: true,
                  alignment: 'center',
                  decoration: 'underline',
                  color: 'skyblue'
                },*/
                /*{
                    image: 'data:image/jpeg;base64,',
                    width: 120,
                  
                  style: 'sectionHeader'
                },*/
                {
                    columns: [
                        /*[
                          {
                            text: this.invoice.customerName,
                            bold:true
                          },
                          { text: this.invoice.address },
                          { text: this.invoice.email },
                          { text: this.invoice.contactNo }
                        ],*/
                        [
                            {
                                text: new Date().getDate() + '/' + month + '/' + new Date().getFullYear(),
                                alignment: 'right'
                            },
                        ]
                    ]
                },
                {
                    text: this.consolidateReport.title + '.' + '' + this.consolidateReport.consolidatedPeriodFinalMonth,
                },
                {
                    table: {
                        headerRows: 1,
                        widths: ['*', 'auto', 'auto', 'auto', 'auto', 'auto', 'auto', 'auto', 'auto', 'auto', 'auto'],
                        body: [
                            ['Desecho', 'I Trim. Recic.', 'I Trim. Recuper.', 'II Trim. Recic.', 'II Trim. Recuper.',
                                'III Trim. Recic.', 'III Trim. Recuper.', 'IV Trim. Recic.', 'IV Trim. Recuper.',
                                'Total Recic.', 'Total Recuper.'],
                            ...this.consolidateReport.details.map(r => ([r.waste, r.irecycledAmount, r.irecoveredAmount,
                                r.iirecycledAmount, r.iirecoveredAmount,
                                r.iiirecycledAmount, r.iiirecoveredAmount,
                                r.ivrecycledAmount, r.ivrecoveredAmount,
                                r.totalRecycledAmount, r.totalRecoveredAmount])),
                        ]
                    }
                },
            ],
            styles: {
                sectionHeader: {
                    bold: true,
                    decoration: 'underline',
                    fontSize: 14,
                    margin: [0, 15, 0, 15]
                }
            }
        };
        if (action === 'download') {
            pdfmake_build_pdfmake__WEBPACK_IMPORTED_MODULE_13___default.a.createPdf(docDefinition).download();
        }
        else if (action === 'print') {
            pdfmake_build_pdfmake__WEBPACK_IMPORTED_MODULE_13___default.a.createPdf(docDefinition).print();
        }
        else {
            pdfmake_build_pdfmake__WEBPACK_IMPORTED_MODULE_13___default.a.createPdf(docDefinition).open();
        }
    }
};
