/* harmony import */ var ngx_toastr__WEBPACK_IMPORTED_MODULE_4__ = __webpack_require__(/*! ngx-toastr */ "5eHb");
/* harmony import */ var src_app_model_reciclaje_waste_entity__WEBPACK_IMPORTED_MODULE_5__ = __webpack_require__(/*! src/app/model/reciclaje/waste.entity */ "F0WS");
/* harmony import */ var src_app_services_reciclaje_waste_service__WEBPACK_IMPORTED_MODULE_6__ = __webpack_require__(/*! src/app/services/reciclaje/waste.service */ "PsH5");
/* harmony import */ var sweetalert2__WEBPACK_IMPORTED_MODULE_7__ = __webpack_require__(/*! sweetalert2 */ "PSD3");
/* harmony import */ var sweetalert2__WEBPACK_IMPORTED_MODULE_7___default = /*#__PURE__*/__webpack_require__.n(sweetalert2__WEBPACK_IMPORTED_MODULE_7__);








let CardAddWasteComponent = class CardAddWasteComponent {
    constructor(_wasteService, toastr) {
        this._wasteService = _wasteService;
        this.toastr = toastr;
        this.showAlert = false;
        this.waste = new src_app_model_reciclaje_waste_entity__WEBPACK_IMPORTED_MODULE_5__["Waste"]();
        this.wastes = [];
        this.toUpdate = false;
        this.wUpdateId = 0;
        //this.downloadPDF();
    }
    ngOnInit() {
        this.findAll();
    }
    downloadPDF() {
        const doc = new jspdf__WEBPACK_IMPORTED_MODULE_3__["default"]();
        doc.text('Hello world!', 10, 10);
        doc.save('hello-world.pdf');
    }
    create() {
        return this._wasteService.create(this.waste).subscribe((res) => {
            this.toastr.success(res.message);
            this.findAll();
            this.waste.name = null;
            this.waste.description = null;
        }, ex => {
            if (ex.error.status === 400) {
                sweetalert2__WEBPACK_IMPORTED_MODULE_7___default.a.fire({
                    icon: 'error',
                    title: 'Oops...',
                    text: 'No puede estar vacío el campo desecho!'
                });
            }
        });
    }
    remove(id) {
        let wDelete = this.wastes[id];
        return this._wasteService.delete(wDelete.code).subscribe((res) => {
            debugger;
            this.wastes = res.obj;
            this.findAll();
        }, ex => {
            if (ex.error.status === 500) {
                sweetalert2__WEBPACK_IMPORTED_MODULE_7___default.a.fire({
                    icon: 'error',
                    title: 'Oops...',
                    text: 'No puede eliminar el desecho por estar asociado algún plan existente!'
                });
            }
        });
    }
    findAll() {
        return this._wasteService.findAll().subscribe((res) => {
            this.wastes = res;
        });
    }
    update(id) {
        this.toUpdate = true;
        let wUpdate = this.wastes[id];
        this.wUpdateId = wUpdate.code;
        this.waste.name = wUpdate.name;
        this.waste.description = wUpdate.description;
    }
    updated(wUpdateId) {
        return this._wasteService.update(wUpdateId, this.waste).subscribe((res) => {
            this.findAll();
            this.toastr.success(res.message);
            this.waste.name = null;
            this.waste.description = null;
            this.toUpdate = false;
        });
    }
};
CardAddWasteComponent.ctorParameters = () => [
    { type: src_app_services_reciclaje_waste_service__WEBPACK_IMPORTED_MODULE_6__["WasteService"] },
    { type: ngx_toastr__WEBPACK_IMPORTED_MODULE_4__["ToastrService"] }
];
CardAddWasteComponent = Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__decorate"])([
    Object(_angular_core__WEBPACK_IMPORTED_MODULE_2__["Component"])({
        selector: "app-card-addwaste",
        template: _raw_loader_card_addwaste_component_html__WEBPACK_IMPORTED_MODULE_1__["default"],
    })
], CardAddWasteComponent);



