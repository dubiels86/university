let HeaderStatsBaseComponent = class HeaderStatsBaseComponent {
    constructor(_fuelService) {
        this._fuelService = _fuelService;
        this.fuelAdd = 0;
        this.fuelRechargued = 0;
        this.fuelTotal = 0;
        this.aux = [];
    }
    ngOnInit() {
        this._fuelService.getFuelTotal().subscribe((res) => {
            this.aux = res;
            this.fuelAdd = this.aux[0];
            this.fuelRechargued = this.aux[1];
            this.fuelTotal = this.aux[2];
        });
    }
};
