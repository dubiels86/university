/* harmony import */ var _raw_loader_card_addoffice_component_html__WEBPACK_IMPORTED_MODULE_1__ = __webpack_require__(/*! raw-loader!./card-addoffice.component.html */ "nn0s");
/* harmony import */ var _angular_core__WEBPACK_IMPORTED_MODULE_2__ = __webpack_require__(/*! @angular/core */ "fXoL");
/* harmony import */ var jspdf__WEBPACK_IMPORTED_MODULE_3__ = __webpack_require__(/*! jspdf */ "i680");
/* harmony import */ var ngx_toastr__WEBPACK_IMPORTED_MODULE_4__ = __webpack_require__(/*! ngx-toastr */ "5eHb");
/* harmony import */ var src_app_model_reciclaje_office_entity__WEBPACK_IMPORTED_MODULE_5__ = __webpack_require__(/*! src/app/model/reciclaje/office.entity */ "dSTD");
/* harmony import */ var src_app_services_reciclaje_office_service__WEBPACK_IMPORTED_MODULE_6__ = __webpack_require__(/*! src/app/services/reciclaje/office.service */ "0RtF");
/* harmony import */ var sweetalert2__WEBPACK_IMPORTED_MODULE_7__ = __webpack_require__(/*! sweetalert2 */ "PSD3");
/* harmony import */ var sweetalert2__WEBPACK_IMPORTED_MODULE_7___default = /*#__PURE__*/__webpack_require__.n(sweetalert2__WEBPACK_IMPORTED_MODULE_7__);








let CardAddOfficeComponent = class CardAddOfficeComponent {
    constructor(_officeService, toastr) {
        this._officeService = _officeService;
        this.toastr = toastr;
        this.showAlert = false;
        this.office = new src_app_model_reciclaje_office_entity__WEBPACK_IMPORTED_MODULE_5__["Office"]();
        this.selectedOffice = '';
        this.offices = [];
        this.toUpdate = false;
        this.officeUpdateId = 0;
        this.isEditable = false;
        //this.downloadPDF();
    }
    ngOnInit() {
        this.findAll();
    }
    downloadPDF() {
        const doc = new jspdf__WEBPACK_IMPORTED_MODULE_3__["default"]();
        doc.text('Hello world!', 10, 10);
        doc.save('hello-world.pdf');
    }
    create() {
        return this._officeService.create(this.office).subscribe((res) => {
            this.toastr.success(res.message);
            this.findAll();
            this.office.acronym = null;
            this.office.name = null;
        }, ex => {
            if (ex.status === 400) {
                this.toastr.error('No puede dejar campos vacíos');
            }
        });
    }
    remove(id) {
        let officeDelete = this.offices[id];
        sweetalert2__WEBPACK_IMPORTED_MODULE_7___default.a.fire({
            title: 'Estás seguro?',
            text: "Desea eliminar la U.O!",
            icon: 'warning',
            showCancelButton: true,
            confirmButtonColor: '#3085d6',
            cancelButtonColor: '#d33',
            confirmButtonText: 'Sí, eliminarla!'
        }).then((result) => {
            if (result.isConfirmed) {
                return this._officeService.delete(officeDelete.acronym).subscribe((res) => {
                    this.findAll();
                    sweetalert2__WEBPACK_IMPORTED_MODULE_7___default.a.fire('Eliminado!', 'La U.O ha sido eliminada.', 'success');
                }, ex => {
                    if (ex.error.status === 500) {
                        sweetalert2__WEBPACK_IMPORTED_MODULE_7___default.a.fire({
                            icon: 'error',
                            title: 'Oops...',
                            text: 'No puede eliminar U.O por estar asociada algún plan existente!'
                        });
                    }
                });
            }
        });
    }
    findAll() {
        return this._officeService.findAll().subscribe((res) => {
            this.offices = res;
        });
    }
    update(id) {
        this.toUpdate = true;
        let fUpdate = this.offices[id];
        this.officeUpdateId = fUpdate.acronym;
        this.office.acronym = fUpdate.acronym;
        this.office.name = fUpdate.name;
        this.isEditable = !this.isEditable;
    }
    updated(officeUpdateId) {
        return this._officeService.update(officeUpdateId, this.office).subscribe((res) => {
            this.findAll();
            this.toastr.success(res.message);
            this.office.acronym = null;
            this.office.name = null;
            this.isEditable = !this.isEditable;
            this.toUpdate = false;
        });
    }
    findOne() {
        const aux = [];
        return this._officeService.findOne(this.selectedOffice).subscribe((res) => {
            aux.push(res);
            this.offices = aux;
        });
    }
};
CardAddOfficeComponent.ctorParameters = () => [
    { type: src_app_services_reciclaje_office_service__WEBPACK_IMPORTED_MODULE_6__["OfficeService"] },
