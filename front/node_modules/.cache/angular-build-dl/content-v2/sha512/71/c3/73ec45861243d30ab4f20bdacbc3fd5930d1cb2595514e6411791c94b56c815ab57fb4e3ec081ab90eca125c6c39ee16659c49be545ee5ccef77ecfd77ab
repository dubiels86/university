/***/ "0wHU":
/*!****************************************************************************************************!*\
  !*** ./src/app/components/cards/Recycle/card-addoffice/card-addoffice/card-addoffice.component.ts ***!
  \****************************************************************************************************/
/*! exports provided: CardAddOfficeComponent */
/***/ (function(module, __webpack_exports__, __webpack_require__) {

"use strict";
__webpack_require__.r(__webpack_exports__);
/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "CardAddOfficeComponent", function() { return CardAddOfficeComponent; });
