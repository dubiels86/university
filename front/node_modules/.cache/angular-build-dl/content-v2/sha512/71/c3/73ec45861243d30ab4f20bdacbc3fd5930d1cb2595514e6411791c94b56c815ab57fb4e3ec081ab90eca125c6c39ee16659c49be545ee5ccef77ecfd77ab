/*!***********************************************************************!*\
  !*** ./src/app/components/cards/card-addcar/card-addcar.component.ts ***!
  \***********************************************************************/
/*! exports provided: CardAddCarComponent */
/***/ (function(module, __webpack_exports__, __webpack_require__) {

"use strict";
__webpack_require__.r(__webpack_exports__);
/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "CardAddCarComponent", function() { return CardAddCarComponent; });
/* harmony import */ var tslib__WEBPACK_IMPORTED_MODULE_0__ = __webpack_require__(/*! tslib */ "mrSG");
/* harmony import */ var _raw_loader_card_addcar_component_html__WEBPACK_IMPORTED_MODULE_1__ = __webpack_require__(/*! raw-loader!./card-addcar.component.html */ "fmAs");
/* harmony import */ var _angular_core__WEBPACK_IMPORTED_MODULE_2__ = __webpack_require__(/*! @angular/core */ "fXoL");
/* harmony import */ var ngx_toastr__WEBPACK_IMPORTED_MODULE_3__ = __webpack_require__(/*! ngx-toastr */ "5eHb");
/* harmony import */ var _app_model_car_entity__WEBPACK_IMPORTED_MODULE_4__ = __webpack_require__(/*! ../../../../app/model/car.entity */ "yUiI");
/* harmony import */ var _app_services_fuel_service__WEBPACK_IMPORTED_MODULE_5__ = __webpack_require__(/*! ../../../../app/services/fuel.service */ "2qI/");






let CardAddCarComponent = class CardAddCarComponent {
    constructor(_fuelService, toastr) {
        this._fuelService = _fuelService;
        this.toastr = toastr;
        this.toUpdate = false;
        this.carUpdateId = 0;
        this.carFilterId = 0;
        this.typevhs = [
            { id: 1, name: 'Camión Plancha' },
            { id: 2, name: 'Camión Furgón' },
            { id: 3, name: 'Omnibus' },
            { id: 4, name: 'Panel Sprinter' },
            { id: 5, name: 'Rastra' },
            { id: 6, name: 'Microbús' },
        ];
        this.car = new _app_model_car_entity__WEBPACK_IMPORTED_MODULE_4__["Car"]();
        this.cars = [];
        this.carsFilter = [];
        this.cc = [];
    }
    ngOnInit() {
        this.loadCars();
    }
    trackBy(index, item) {
        return item.id;
    }
    selected() {
        this.selectedCar = this.selectedCar;
    }
    createCar() {
        this.car.type = this.selectedCar;
        this.car.mat = this.car.mat.toString();
        return this._fuelService.createCar(this.car).subscribe((res) => {
            this.loadCars();
            this.toastr.success(res.message);
            this.car.mat = '';
            this.car.type = null;
            this.car.driver = '';
            this.car.ic = null;
            this.car.ci = '';
            this.toUpdate = false;
        });
    }
    showToaster() {
        this.toastr.success("Hello, I'm the toastr message.");
    }
    removeCar(id) {
        let carDelete = this.cars[id];
        return this._fuelService.deleteCar(carDelete.id).subscribe((res) => {
            this.loadCars();
        });
    }
    updatedCar(carUpdateId) {
        this.car.type = this.selectedCar;
        return this._fuelService.updateCar(carUpdateId, this.car).subscribe((res) => {
            this.loadCars();
            this.toastr.success(res.message);
            this.car.mat = '';
            this.car.type = null;
            this.car.driver = '';
            this.car.ic = null;
            this.car.ci = '';
            this.car.capTanq = null;
            this.toUpdate = false;
        });
    }
    updateCar(id) {
        this.toUpdate = true;
        let carUpdate = this.cars[id];
        this.carUpdateId = carUpdate.id;
        this.car.mat = carUpdate.mat;
        this.selectedCar = carUpdate.type;
        this.car.driver = carUpdate.driver;
        this.car.ci = carUpdate.ci;
        this.car.ic = carUpdate.ic;
        this.car.capTanq = carUpdate.capTanq;
    }
    loadCars() {
        return this._fuelService.getCar().subscribe((res) => {
            this.cars = res;
            this.carsFilter = res;
        });
    }
    filterCar() {
        this.cars = [];
        this.cc = [];
        return this._fuelService.getCarById(this.carFilterId).subscribe((res) => {
            this.cc.push(res);
            this.cars = this.cc;
        });
    }
};
CardAddCarComponent.ctorParameters = () => [
    { type: _app_services_fuel_service__WEBPACK_IMPORTED_MODULE_5__["FuelService"] },
    { type: ngx_toastr__WEBPACK_IMPORTED_MODULE_3__["ToastrService"] }
];
CardAddCarComponent = Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__decorate"])([
    Object(_angular_core__WEBPACK_IMPORTED_MODULE_2__["Component"])({
        selector: "app-card-addcar",
        template: _raw_loader_card_addcar_component_html__WEBPACK_IMPORTED_MODULE_1__["default"]
    })
], CardAddCarComponent);



/***/ }),

