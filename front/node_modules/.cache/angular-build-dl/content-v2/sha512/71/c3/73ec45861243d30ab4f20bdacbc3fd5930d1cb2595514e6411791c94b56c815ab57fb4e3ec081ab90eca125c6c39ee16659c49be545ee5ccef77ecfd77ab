    createPlan() {
        this.details = [];
        let wasteAux;
        for (let index = 0; index < this.wastes.length; index++) {
            const element = this.wastes[index];
            wasteAux = {
                waste: {
                    code: element.code,
                    name: element.name,
                    description: element.description
                }
            };
            this.details.push(wasteAux);
        }
        this.plan.details = this.details;
        sweetalert2__WEBPACK_IMPORTED_MODULE_13___default.a.fire({
            title: 'Desea crear el nuevo plan?',
            showDenyButton: true,
            showCancelButton: true,
            confirmButtonText: `Crear`,
            denyButtonText: `No crear`,
        }).then((result) => {
            /* Read more about isConfirmed, isDenied below */
            if (result.isConfirmed) {
                return this._planService.create(this.plan, this.officePlan).subscribe((res) => {
                    sweetalert2__WEBPACK_IMPORTED_MODULE_13___default.a.fire('Creado!', '', 'success');
                    this.plan = res.obj;
                    if (!this.plan.confirmed) {
                        this.isConfirmedPlan = false;
                    }
                    else {
                        this.isConfirmedPlan = true;
                    }
                    this.detailsActual = this.plan.details;
                    this.planToUpdate = !this.planToUpdate;
                    this.isVisibleConfirmedPlan = true;
                    // this.toastr.success(res.message);
                }, ex => {
                    if (ex.status === 400 || ex.status === 500) {
                        this.toastr.error('Puede que algunos de sus campos esten vacíos');
                    }
                });
            }
            else if (result.isDenied) {
                sweetalert2__WEBPACK_IMPORTED_MODULE_13___default.a.fire('Los cambios no han sido salvados', '', 'info');
            }
        });
    }
    updatePlan() {
        const swalWithBootstrapButtons = sweetalert2__WEBPACK_IMPORTED_MODULE_13___default.a.mixin({
            customClass: {
                confirmButton: 'btn btn-success',
                cancelButton: 'btn btn-danger'
            },
            buttonsStyling: true
        });
        if (this.plan.estimation < 0) {
            sweetalert2__WEBPACK_IMPORTED_MODULE_13___default.a.fire({
                icon: 'error',
                title: 'Oops...',
                text: 'El valor de la estimación no puede ser negativo!'
            });
        }
        else {
            swalWithBootstrapButtons.fire({
                title: 'Estás seguro?',
                text: "Actualizar el Plan!",
                icon: 'warning',
                showCancelButton: true,
                confirmButtonText: 'Sí, actualizarlo!',
                cancelButtonText: 'No, cancelar!',
                reverseButtons: true
            }).then((result) => {
                if (result.isConfirmed) {
                    if (this.yearUpdate === this.plan.year) {
                        this.plan.details = this.detailsActual;
                        this._planService.update(this.plan.id, this.plan, this.officePlan).subscribe((res) => {
                            this.planToUpdate = !this.planToUpdate;
                            this.plan = res.obj;
                            debugger;
                            if (!this.plan.confirmed) {
                                this.isConfirmedPlan = false;
                            }
                            else {
                                this.isConfirmedPlan = true;
                            }
                            this.detailsActual = [];
                            this.plan.year = null;
                            this.officePlan = null;
                            this.plan.estimation = null;
                            this.plan.draftedBy = '';
                            this.plan.approvedBy = '';
                            this.plan.positionApprovedBy = null;
                            this.plan.positionDraftedBy = null;
                            this.isVisibleConfirmedPlan = false;
                            //this.detailsActual = this.plan.details;
                            console.log(this.plan);
                            this.toastr.success(res.message);
                        }, ex => {
                            if (ex.status === 400) {
                                this.toastr.error(ex.error.error);
                            }
                            if (ex.status === 401) {
                                this.toastr.error(ex.error.error);
                            }
                        });
                    }
                    else {
                        this.toastr.error('No coincide el año y/o la U.O a actualizar');
                    }
                }
                else if (
                /* Read more about handling dismissals below */
                result.dismiss === sweetalert2__WEBPACK_IMPORTED_MODULE_13___default.a.DismissReason.cancel) {
                    swalWithBootstrapButtons.fire('Cancelado', 'Los datos del plan no han sido actualizados :)', 'error');
                }
            });
        }
    }
    confirmPlan() {
        return this._planService.confirm(this.plan.id, this.officePlan).subscribe((res) => {
            debugger;
            this.isConfirmedPlan = !this.isConfirmedPlan;
            console.log(res);
            this.plan.confirmed = res.confirmed;
            this.toastr.success(res.message);
            this.findAllPlans();
        });
    }
    unconfirmPlan() {
        return this._planService.unconfirm(this.plan.id, this.officePlan).subscribe((res) => {
            debugger;
            this.isConfirmedPlan = !this.isConfirmedPlan;
            this.plan.confirmed = res.confirmed;
            this.toastr.success(res.message);
            this.findAllPlans();
        });
    }
    //Generate PDF Report/////
    generateConsolidatedPlan() {
        return this._planService.reportingGenerate(this.yearConsolidatedPlan).subscribe((res) => {
            this.consolidatePlan.approvedBy = res.approvedBy;
            this.consolidatePlan.description = res.description;
            this.consolidatePlan.details = res.details;
