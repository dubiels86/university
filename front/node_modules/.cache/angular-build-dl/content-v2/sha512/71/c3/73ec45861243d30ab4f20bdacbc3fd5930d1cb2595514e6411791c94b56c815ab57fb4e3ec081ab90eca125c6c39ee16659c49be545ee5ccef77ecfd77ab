/* harmony import */ var _angular_common_http__WEBPACK_IMPORTED_MODULE_2__ = __webpack_require__(/*! @angular/common/http */ "tk/3");
/* harmony import */ var _config_config__WEBPACK_IMPORTED_MODULE_3__ = __webpack_require__(/*! ../config/config */ "0np6");




let FuelService = class FuelService {
    constructor(httpClient) {
        this.httpClient = httpClient;
        this.url = _config_config__WEBPACK_IMPORTED_MODULE_3__["Configuration"].apiEndpoint + 'entrada/';
        this.API = 'http://localhost:3000/api/fuels';
        this.APICAR = 'http://localhost:3000/api/cars';
    }
    getFuel() {
        return this.httpClient.get(this.API);
    }
    getFuelById(id) {
        return this.httpClient.get(`${this.API}/${id}`);
    }
    getFuelMatDate(id, date1, date2) {
        return this.httpClient.get(`${this.API}/byCarAndDate/${id}/${date1}/${date2}`);
    }
    getStatsByCar(id, date1, date2) {
        return this.httpClient.get(`${this.API}/getStatsByCar/${id}/${date1}/${date2}`);
    }
    getByMonths() {
        return this.httpClient.get(`${this.API}/byMonths/${2021}`);
    }
    getFuelTotal() {
        return this.httpClient.get(`${this.API}`);
    }
    createFuel(fuel) {
        return this.httpClient.post(this.API, fuel);
    }
    deleteFuel(id) {
        return this.httpClient.delete(`${this.API}/${id}`);
    }
    updateFuel(id, fuel) {
        return this.httpClient.patch(`${this.API}/${id}`, fuel);
    }
    getCar() {
        return this.httpClient.get(this.APICAR);
    }
    getCarById(id) {
        return this.httpClient.get(`${this.APICAR}/${id}`);
    }
    createCar(car) {
        return this.httpClient.post(this.APICAR, car);
    }
    deleteCar(id) {
        return this.httpClient.delete(`${this.APICAR}/${id}`);
    }
    updateCar(id, car) {
        return this.httpClient.patch(`${this.APICAR}/${id}`, car);
    }
};
